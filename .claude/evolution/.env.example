# Langfuse Configuration
# Copy to .env and fill in your values

# Auth Secrets (Generate secure random strings)
# Example: openssl rand -base64 32
NEXTAUTH_SECRET=your-nextauth-secret-here-minimum-32-characters
SALT=your-salt-secret-here-minimum-32-characters

# Admin User (First-time setup)
LANGFUSE_ADMIN_EMAIL=admin@localhost
LANGFUSE_ADMIN_PASSWORD=changeme

# Organization
LANGFUSE_ORG_NAME=claude01
LANGFUSE_PROJECT_NAME=agent-evolution

# Telemetry (optional)
TELEMETRY_ENABLED=true

# API Keys (Get from Langfuse dashboard after first login)
# Settings → API Keys → Create new key
LANGFUSE_PUBLIC_KEY=pk-lf-...
LANGFUSE_SECRET_KEY=sk-lf-...
LANGFUSE_HOST=http://localhost:3000
