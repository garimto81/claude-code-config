# Photo Factory - Environment Variables
# Copy this file to .env and fill in your actual values

# IMPORTANT: Vite requires VITE_ prefix for client-side env variables
# See: https://vitejs.dev/guide/env-and-mode.html

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_anon_public_key_here

# Cloudinary API Configuration
# Option 1: Unsigned Upload (프론트엔드 직접 업로드 - 현재 사용 중)
VITE_CLOUDINARY_CLOUD_NAME=your_cloud_name_here
VITE_CLOUDINARY_UPLOAD_PRESET=your_upload_preset_here

# Option 2: Signed Upload (서버 측 업로드 - 고급 기능용, 선택사항)
# VITE_CLOUDINARY_API_KEY=your_api_key_here
# VITE_CLOUDINARY_API_SECRET=your_api_secret_here

# Development Server (optional)
PORT=8080
